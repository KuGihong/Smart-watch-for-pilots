<launch>
  <!-- 아두이노 노드 실행 -->
  <node pkg="rosserial_python" type="serial_node.py" name="arduino_node">
    <param name="port" value="/dev/ttyACM0" /> <!-- 아두이노의 포트에 맞게 변경 -->
    <param name="baud" value="57600" />       <!-- 아두이노와 통신 속도에 맞게 변경 -->
  </node>

  <!-- heartRate_rosserial.py 노드 실행 -->
  <node pkg="your_package_name" type="heartRate_rosserial.py" name="heart_rate_node" output="screen">
  </node>
  
</launch>

